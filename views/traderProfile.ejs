    <div class="form-popup" id="myForm">
        <form action="/api/copyDetail" class="form-container" method="post">
            <h1 style="text-align:center">Copy</h1>
            <p>Select Copy Type: </p>
            <input type="radio" name="orderType" value="Regular" id="Regular-Order">
            <label for="Regular-Order">Regular Order</label>

            <input type="radio" name="orderType" value="Bracket" id="Bracket-Order">
            <label for="Bracket-Order">Bracket Order</label><br><br>

            <p>Amount willing to invest</p>
            <label for="amount-invested"><b>Amount willing to invest</b></label>
            <input type="text" placeholder="Enter Amount" name="amountInvest" id="amount-invested"><br>

            <div class="bracket-type">

                <!-- <input type="text" name="" id=""> -->
                <div class="stoploss">
                    <p>Stoploss</p>
                    <hr><br>

                    <!-- <h1>Range Slider Picture</h1> -->

                    <div class="slidecontainer">
                        <input type="range" min="0" max="100" value="50" step="0.1" class="slider1 " id="stoploss-Range">
                        <p>Percent: <span id="per1"></span></p>
                    </div>



                    <!-- <label for="percent"><b>%</b></label> -->
                    <input type="text" placeholder="BY Percentage" id="Stoploss-percent"><br>

                    <!-- <label for="amount"><b>$</b></label> -->
                    <input type="text" placeholder="BY Amount" name="stoploss" id="Stoploss-amount">
                </div>

                <br>
                <div class="stopgain">
                    <p>Stopgain</p>
                    <hr><br>
                    <!-- <input type="text" name="" id=""> -->
                    <!-- <label for="percent"><b>%</b></label> -->
                    <div class="slidecontainer">
                        <input type="range" min="0" max="100" value="50" step="0.1" class="slider2" id="stopgain-Range">
                        <p>Percent: <span id="per2"></span></p>
                    </div>
                    <input type="text" placeholder="BY Percentage" id="Stopgain-percent"><br>

                    <!-- <label for="amount"><b>$</b></label> -->
                    <input type="text" placeholder="BY Amount" name="stopgain" id="Stopgain-amount">
                </div>
            </div>

            <br>


            <button type="submit" class="btn">Copy</button><br>
            <button type="button" class="btn cancel" onclick="closeForm()">Close</button>
        </form>
    </div>



